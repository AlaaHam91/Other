@model IEnumerable<string>
    <div data-role="navbar">
<ul>
    @foreach (var link in Model)
    {
<li>
    @Html.RouteLink(link,new {
    controller = "Book",
    action = "List",
    specilization = link,
    pagenum = 1

},
        new { data_transition="fade"
        ,@class= (ViewBag.SelectedSpec == link) ? "ui-btn-active" :null
        })

</li>
    }
</ul>
</div>