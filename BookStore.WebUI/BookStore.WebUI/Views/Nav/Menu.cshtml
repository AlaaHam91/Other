@model IEnumerable<string>
@Html.ActionLink("Home", "List", "Book", null, new { @class = "btn btn-block btn-default btn-lg" })
@foreach (var link in Model)
{
    @Html.RouteLink(link,
        new
        {
            controller = "Book",
            action = "List",
            specilization = link,
            pagenum = 1
        }, new { @class = "btn btn-block btn-default btn-lg " + ( (ViewBag.SelectedSpec == link) ? "btn-primary" : "").ToString()})




}